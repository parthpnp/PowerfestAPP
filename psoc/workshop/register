      <script>

      var event_name = 'PSoC';

      


               $(document).ready(function(){
              //document.getElementById('form1').innerHTML=generate_form(1,'compi');
			  
            });
            var users;

            
            $("#team_members").change(function(){
              $('#diverge').hide();
                $('#reg').show();
               var formcount=document.getElementById('team_members').value;
              document.getElementById('forms').innerHTML='';
              for(var i=1;i<=formcount;i++){
                 document.getElementById('forms').innerHTML+=new_old(i)+new_email(i);
            }
            });

            function create_forms(){
              var formcount=document.getElementById('team_members').value;
              document.getElementById('forms').innerHTML='';
              for(var i=1;i<=formcount;i++){
                 document.getElementById('forms').innerHTML+=new_old(i)+new_email(i);
            }
             }

            function new_old(n){
                if(n==1 || n=='1'){
                    var heading='<h6 class="team_leader">Team Leader</h6><br />';
                }
                else{
                    var heading='<h6 class="team_leader">Participant '+n+'</h6><br />';
                }
                var new_old='<input type="radio" id="user' + n + '1" name="user' + n + '" style="display:none" checked="checked" onclick="replace(' + n + ',1)" />';
                var div_open='<div id="section' + n + '">';
                
                return heading+new_old+div_open;
            }

            function exist_email(n){
                var email='<input type="email" name="emailid' + n + '" required="required" placeholder="Email Id" id="email' + n + '" class="old" title="Email Id" size="30" />';
                return email;
            }

            if (event_name != 'Robowars' && event_name != 'IRC Egypt') {

              function new_email(n){
              var firstname='<input type="text" name="firstname' + n + '" required="required" placeholder="First Name" id="fn" pattern="[A-Za-z ]{1,30}" title="First Name: Only Letters and Spaces" size="30"/>';

              var lastname='<input type="text" name="lastname' + n + '" required="required" placeholder="Last name" id="lln" pattern="[A-Za-z ]{1,30}" title="Last Name: Only Letters and Spaces" size="30"/>';
      
              var email='<input type="email" name="emailid' + n + '" required="required" placeholder="E-Mail" id="email' + n + '" class="new" title="Email Id" size="30"/>';
      
             // var password='<input type="password" name="password' + n + '" required="required" placeholder="Password" id="password' + n + '" class="new" title="Password" size="30" oninput="passwordvalid('+n+')"/>';
            //  var pass_conf='<input type="password" name="pass_conf' + n + '" required="required" placeholder="Confirm Password" id="pass_conf' + n + '" class="new" title="Password" size="30" oninput="passwordvalid('+n+')"/>';
              var no_city='<p class="college_text">Please select "Other" if your city/college does not appear in the list</p></br>';
              
