            function replace(n,type){
              if(type==1){
                  document.getElementById('section'+n).innerHTML=new_email(n);
              }
              else{
                  document.getElementById('section'+n).innerHTML=exist_email(n);
              }
            }

            function change(n){
              var city=document.getElementById('city'+n).value;
   
                  var xmlhttp;
                  
                  if(window.XMLHttpRequest){// code for IE7+, Firefox, Chrome, Opera, Safari
                      xmlhttp=new XMLHttpRequest();
                  }
                  else{// code for IE6, IE5
                      xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
                  }
                  xmlhttp.onreadystatechange=function(){
                      if (xmlhttp.readyState==4 && xmlhttp.status==200){
                          var msg=xmlhttp.responseText;
                          msg+="10000#Other*";
                          document.getElementById('college'+n).innerHTML='';    /// delete every college exisitng
                        colleges=msg.split('*');    //separate college names......
    
                        for(col in colleges){
                          if(colleges[col]!= ''){
                            cols=colleges[col].split('#');
                            document.getElementById('college'+n).innerHTML+='<option value="'+cols[0]+'">'+cols[1]+'</option>';   //append colleges of that city
                          }
                        }
                      }
                  }
                  xmlhttp.open("http://www.techfest.org/psoc/workshop/GET","http://www.techfest.org/register/colleges/"+city,true);
                  xmlhttp.send();
            }

